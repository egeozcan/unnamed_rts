{
  "name": "unnamed-rts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "manipulate-state": "npx -y tsx src/scripts/manipulate_state.ts",
    "ai:new": "npx -y tsx src/scripts/create-ai-implementation.ts",
    "ai:simulate": "npx -y tsx src/scripts/simulate_ai.ts",
    "ai:bench": "npx -y tsx src/scripts/simulate_ai.ts --benchmark",
    "debug": "npx tsx src/scripts/debug/index.ts",
    "docs:dev": "vitepress dev docs/site",
    "docs:build": "vitepress build docs/site",
    "docs:preview": "vitepress preview docs/site"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^1.1.0",
    "jsdom": "^27.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitepress": "^1.6.4",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "zod": "^4.2.1"
  }
}
